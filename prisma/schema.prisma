generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Interview {
  id        String   @id @default(uuid())
  type      String?
  techstack String?
  role      String?
  level     String?
  amount    Int?
  status    String?
  userId    String
  questions String[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
